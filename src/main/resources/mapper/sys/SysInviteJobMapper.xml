<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cy.sys.dao.SysInviteJobDao">
	<!--添加应聘人信息 -->
	<insert id="insertApplicantMes">
		insert into sys_invitejob
		(id,name,sex,age,born,job,specialty,experience,Culture,afterSchool,tel,address,createtime,content)
		values
		(#{id},#{name},#{sex},#{age},#{born},#{job},#{specialty},#{experience},#{Culture},#{afterSchool},
		#{tel},#{address},#{createtime},#{content})
	</insert>



</mapper>

